<!DOCTYPE html>
<html lang="en">
<head>
    <title>DB Info</title>
    <meta charset="utf-8">
</head>
<body>
  <h1>Database Tables</h1>
  <ul>
  <% tables.forEach(function(r) { %>
    <li><%=r.table %>: <%=r.column %> (<%= r.type%>)</li>
  <%}); %>
  </ul>
  <h1>Observations</h1>
  <ul>
    <% obs.forEach(function(r) { %>
      <li><%= r.name %> for <%= r.duration.hours || "0"%> hours <%= r.duration.minutes || "0"%> minutes <%= r.duration.seconds || "0"%> seconds</li>
    <% }); %>
  </ul>
</body>
</html>